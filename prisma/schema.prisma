datasource db {
  provider = "mysql"
  url = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id Int @id @default(autoincrement())
  email String
  name String
  secret String

  gym Academy @relation(fields: [gym_id],references: [id])
  gym_id Int
}

model Academy {
  id Int @id @default(autoincrement())
  name String

  students User[]
}